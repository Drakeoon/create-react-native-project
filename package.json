{
  "name": "create-react-native-project",
  "version": "0.4.2",
  "license": "MIT",
  "description": "React-Native project generator with preconfigured options.",
  "repository": {
    "type": "git",
    "url": "https://github.com/dsznajder/create-react-native-project.git"
  },
  "bugs": {
    "url": "https://github.com/dsznajder/create-react-native-project/issues"
  },
  "homepage": "https://github.com/dsznajder/create-react-native-project",
  "keywords": [
    "react-native",
    "template",
    "project",
    "ios",
    "android"
  ],
  "main": "lib/index.js",
  "bin": {
    "create-react-native-project": "bin/create-react-native-project"
  },
  "files": [
    "lib",
    "bin",
    "templates"
  ],
  "engines": {
    "node": ">= 12.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "prepare": "babel --extensions .ts,.tsx src --out-dir lib --ignore '**/__tests__/**' --source-maps --delete-dir-on-start",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "postinstall": "husky install",
    "typescript": "tsc --noEmit"
  },
  "dependencies": {
    "chalk": "4.1.0",
    "ejs": "3.1.6",
    "execa": "5.0.0",
    "fs-extra": "9.1.0",
    "prompts": "2.4.0",
    "yargs": "16.2.0"
  },
  "devDependencies": {
    "@babel/cli": "7.13.14",
    "@babel/core": "7.13.14",
    "@babel/preset-env": "7.13.12",
    "@babel/preset-typescript": "7.13.0",
    "@types/chalk": "2.2.0",
    "@types/ejs": "3.0.6",
    "@types/fs-extra": "9.0.9",
    "@types/prompts": "2.0.10",
    "@types/yargs": "16.0.1",
    "eslint": "7.23.0",
    "eslint-config-satya164": "3.1.10",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "husky": "6.0.0",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "typescript": "4.2.3"
  },
  "prettier": {
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all",
    "useTabs": false
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint"
    ]
  }
}
