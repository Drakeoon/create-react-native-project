{
  "name": "TemplateProject",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "yarn react-native run-android",
    "clean:all": "yarn clean:watchman && yarn clean:builds && yarn clean:gradlew && yarn clean:cache && yarn clean:packages",
    "clean:builds": "rm -rf ios/build android/build android/app/build",
    "clean:cache": "rm -rf $TMPDIR/react-* $TMPDIR/haste-map-react-native-packager-* ~/.rncache",
    "clean:gradlew": "cd android && ./gradlew clean && cd ..",
    "clean:packages": "rm -rf ios/Pods && rm -rf node_modules && yarn install",
    "clean:watchman": "watchman watch-del-all",
    "coverage": "open coverage/lcov-report/index.html",
    "fastlane:android": "yarn jetify; cd android && bundle update && bundle exec fastlane",
    "fastlane:ios": "cd ios && bundle update && bundle exec fastlane",
    "generate:apk": "cd android; ./gradlew assembleRelease; cd ..; open ./android",
    "ios": "yarn react-native run-ios --simulator='iPhone 11 Pro'",
    "lint": "esw . --ext .js,.ts,.tsx -c .eslintrc.js --color",
    "lint:watch": "yarn lint -w",
    "postinstall": "patch-package && ./scripts/postinstall.sh",
    "stacktrace": "npx metro-symbolicate android/app/build/generated/sourcemaps/react/release/index.android.bundle.map < stacktrace.txt",
    "start": "react-native start",
    "test": "jest --coverage --runInBand --detectOpenHandles --forceExit",
    "test:unit": "jest -u --runInBand",
    "test:watch": "jest --watch",
    "translations": "NODE_ENV=translations babel -f babel.config.js 'src/**/*.{js,jsx,ts,tsx}' --ignore 'src/**/*.test.{js,jsx,ts,tsx}' > translations-output.log",
    "typescript": "tsc --noEmit"
  },
  "dependencies": {
    "@react-native-community/masked-view": "0.1.10",
    "@react-native-community/netinfo": "5.9.7",
    "@react-navigation/native": "5.8.0",
    "formik": "2.2.1",
    "react": "16.13.1",
    "react-native": "0.63.3",
    "react-native-config": "1.3.3",
    "react-native-gesture-handler": "1.8.0",
    "react-native-keyboard-manager": "4.0.13-16",
    "react-native-reanimated": "2.0.0-alpha.7",
    "react-native-redash": "15.11.1",
    "react-native-screens": "2.11.0",
    "yup": "0.29.3"
  },
  "devDependencies": {
    "@babel/cli": "7.12.1",
    "@babel/core": "7.12.3",
    "@babel/runtime": "7.12.1",
    "@graphql-codegen/cli": "1.18.0",
    "@graphql-codegen/introspection": "1.18.0",
    "@graphql-codegen/near-operation-file-preset": "1.17.11",
    "@graphql-codegen/typescript": "1.17.11",
    "@graphql-codegen/typescript-operations": "1.17.8",
    "@release-it/conventional-changelog": "2.0.0",
    "@types/jest": "26.0.15",
    "@types/lodash": "4.14.162",
    "@types/react": "16.9.53",
    "@types/react-native": "0.63.27",
    "@types/react-native-vector-icons": "6.4.6",
    "@types/react-redux": "7.1.9",
    "@types/react-test-renderer": "16.9.3",
    "@types/yup": "0.29.8",
    "babel-jest": "26.6.0",
    "babel-minify": "0.5.1",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-module-resolver": "4.0.0",
    "eslint": "7.11.0",
    "eslint-config-satya164": "3.1.8",
    "eslint-plugin-graphql": "4.0.0",
    "eslint-plugin-simple-import-sort": "5.0.3",
    "husky": "4.3.0",
    "jest": "26.6.0",
    "lint-staged": "10.4.2",
    "metro-react-native-babel-preset": "0.63.0",
    "patch-package": "6.2.2",
    "prettier": "2.1.2",
    "react-native-flipper": "0.62.0",
    "react-test-renderer": "16.13.1",
    "redux-flipper": "1.4.0",
    "release-it": "14.2.0",
    "typescript": "4.0.3",
    "unfetch": "4.2.0"
  },
  "prettier": {
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "parser": "typescript",
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all",
    "useTabs": false
  },
  "jest": {
    "preset": "react-native",
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/src/assets/"
    ],
    "setupFiles": [],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base)"
    ],
    "testPathIgnorePatterns": [
      "__e2e__",
      "node_modules"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn typescript"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --ext ',.ts,.tsx,.js'"
    ]
  }
}
