{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "~/assets/*": ["src/assets/*"],
      "~/components/*": ["src/components/*"],
      "~/helpers/*": ["src/helpers/*"],
      "~/locales/*": ["src/locales/*"],
      "~/scenes/*": ["src/scenes/*"],
      "~/services/*": ["src/services/*"],
      "~/styles/*": ["src/styles/*"],
      "~/types/*": ["src/types/*"]
    },
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "jsx": "react-native",
    "module": "esnext",
    "moduleResolution": "node",
    "noEmit": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "resolveJsonModule": true,
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "target": "esnext"
  },
  "exclude": ["node_modules", "e2e", "jest", "**/__tests__/*"],
  "compileOnSave": false
}
