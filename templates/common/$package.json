{
  "name": "<%- project.name %>",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "yarn react-native run-android",
    "clean:all": "yarn clean:watchman && && yarn clean:pods yarn clean:builds && yarn clean:gradlew && yarn clean:cache && yarn clean:packages",
    "clean:builds": "rm -rf ios/build android/build android/app/build",
    "clean:cache": "rm -rf $TMPDIR/react-* $TMPDIR/haste-map-react-native-packager-* ~/.rncache",
    "clean:gradlew": "cd android && ./gradlew clean && cd ..",
    "clean:packages": "rm -rf node_modules && yarn install",
    "clean:pods": "rm -rf ios/Pods ios/Podfile.lock",
    "clean:watchman": "watchman watch-del-all",
    "coverage": "open coverage/lcov-report/index.html",
    "fastlane:android": "yarn jetify; cd android && bundle exec fastlane; cd ..",
    "fastlane:ios": "cd ios && bundle exec fastlane; cd ..",
    "generate:apk": "cd android; ./gradlew assembleRelease; cd ..; open ./android",
    "generate:types": "graphql-codegen",
    "ios": "yarn react-native run-ios --simulator='iPhone 12'",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "lint:watch": "yarn lint -w",
    "postinstall": "./scripts/postinstall.sh",
    "start": "react-native start",
    "test": "jest --coverage --runInBand --detectOpenHandles --forceExit",
    "test:unit": "jest -u --runInBand",
    "test:watch": "jest --watch",
    "typescript": "tsc --noEmit"
  },
  "dependencies": {
    "@apollo/client": "3.3.11",
    "@react-native-community/masked-view": "0.1.10",
    "@react-native-community/netinfo": "6.0.0",
    "@react-navigation/native": "5.9.3",
    "graphql": "15.5.0",
    "react": "16.13.1",
    "react-native": "0.63.4",
    "react-native-config": "1.4.2",
    "react-native-gesture-handler": "1.10.3",
    "react-native-keyboard-manager": "4.0.13-17",
    "react-native-reanimated": "2.0.0",
    "react-native-redash": "16.0.8",
    "react-native-safe-area-context": "3.2.0",
    "react-native-screens": "2.18.1"
  },
  "devDependencies": {
    "@babel/cli": "7.13.10",
    "@babel/core": "7.13.10",
    "@babel/runtime": "7.13.10",
    "@graphql-codegen/cli": "1.21.2",
    "@graphql-codegen/introspection": "1.18.1",
    "@graphql-codegen/near-operation-file-preset": "1.17.13",
    "@graphql-codegen/typescript": "1.21.1",
    "@graphql-codegen/typescript-operations": "1.17.15",
    "@types/jest": "26.0.20",
    "@types/lodash": "4.14.168",
    "@types/react": "17.0.3",
    "@types/react-native": "0.63.51",
    "@types/react-test-renderer": "17.0.1",
    "babel-jest": "26.6.3",
    "babel-minify": "0.5.1",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-module-resolver": "4.1.0",
    "eslint": "7.21.0",
    "eslint-config-satya164": "3.1.9",
    "eslint-plugin-graphql": "4.0.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "husky": "5.1.3",
    "jest": "26.6.3",
    "lint-staged": "10.5.4",
    "metro-react-native-babel-preset": "0.65.2",
    "prettier": "2.2.1",
    "react-native-flipper": "0.79.1",
    "react-test-renderer": "16.13.1",
    "typescript": "4.2.3",
    "unfetch": "4.2.0"
  },
  "prettier": {
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "parser": "typescript",
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all",
    "useTabs": false
  },
  "jest": {
    "preset": "react-native",
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/src/assets/"
    ],
    "setupFiles": [],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base)"
    ],
    "testPathIgnorePatterns": [
      "__e2e__",
      "node_modules"
    ]
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint"
    ]
  }
}
